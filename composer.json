{
    "name": "phlexible/indexer-page-media-bundle",
    "description": "phlexible indexer page media bundle",
    "license": "MIT",
    "type": "symfony-bundle",
    "keywords": ["indexer", "search"],
    "autoload": {
        "psr-4": {
            "Phlexible\\Bundle\\IndexerPageMediaBundle\\": ""
        }
    },
    "require": {
        "php": ">=5.6.0",
        "phlexible/indexer-bundle": "^1.1.4",
        "phlexible/indexer-page-bundle": "^1.2.3",
        "phlexible/indexer-media-bundle": "^1.0.2",
        "phlexible/elastica-bundle": "^1.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7",
        "symfony/config": "~2.8",
        "symfony/dependency-injection": "~2.8",
        "symfony/http-kernel": "~2.8"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
